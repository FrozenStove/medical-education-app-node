{
    "name": "medical-education-server",
    "version": "1.0.0",
    "description": "Medical Education Assistant Backend",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "test": "jest",
        "ingest": "ts-node -r dotenv/config src/scripts/ingestArticles.ts",
        "verify": "ts-node src/scripts/verifyIngestion.ts",
        "check": "ts-node src/scripts/checkChroma.ts"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "langchain": "^0.1.0",
        "openai": "^4.20.0",
        "chromadb": "^1.7.3",
        "zod": "^3.22.4",
        "config": "~3.3",
        "pdf-parse": "^1.1.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "@types/cors": "^2.8.17",
        "@types/pdf-parse": "^1.1.4",
        "typescript": "^5.3.2",
        "ts-node-dev": "^2.0.0",
        "@types/jest": "^29.5.10",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1"
    }
}